        "def plot_crimes_by_year(years=[2017,2016,2015], crimes=\"Larceny Offenses\", density=0.008, size=1):\n",
        "    plt.scatter(\n",
        "        x=merged[\"Open Data X\"].where(\n",
        "          ((merged[\"Year\"].isin(years)) & (merged[\"Offense Category\"].isin(crimes)))\n",
        "        ), \n",
        "        y=merged[\"Open Data Y\"].where(\n",
        "          ((merged[\"Year\"].isin(years)) & (merged[\"Offense Category\"].isin(crimes)))\n",
        "        ), \n",
        "        s=size, color=\"b\", \n",
        "        alpha=density, \n",
        "        zorder=3\n",
        "    )\n",
        "    plt.show()"
      ],
      "execution_count": 0,
      "outputs": []
    },
